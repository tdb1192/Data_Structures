public class TestLinkedList {
	
	public static void main(String[] args) {
		
		Node first = new Node("A");
		Node second = new Node("B");
		Node third = new Node("C");
		Node fourth = new Node("D");
		Node fifth = new Node("E");

		LinkedList link = new LinkedList(third);
			link.printList();	// 3
		link.removeFirst();		
			link.printList();	// empty
		link.addLast(third);
			link.printList(); 	// 3
		link.removeLast();
			link.printList();	// empty
		link.addFirst(third);
			link.printList();	// 3
		link.addLast(fourth);
			link.printList();	// 3 -> 4
		link.addFirst(second);
			link.printList();	// 2 -> 3 -> 4
		link.addFirst(first);
			link.printList();	// 1-> 2 -> 3 -> 4
		link.addLast(fifth);
			link.printList();	// 1 -> 2 -> 3 -> 4 -> 5
		link.removeFirst();
			link.printList();	// 2 -> 3 -> 4 -> 5
		link.removeLast();
			link.printList();	// 2 -> 3 -> 4
		link.removeLast();
			link.printList();	// 2 -> 3
		link.removeFirst();	
			link.printList();	// 3
	}
	
}
